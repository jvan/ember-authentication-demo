
<h2>admin</h2>

<div class="list-group">
{{#each}}
   <div class="list-group-item">
      <h4>{{title}} <small>{{author}}</small>
      {{#if published}}
         <span class="label label-success pull-right">published</span>
      {{else}}
         <span class="label label-warning pull-right">draft</span>
      {{/if}} 
      </h4>
   </div>
{{/each}}
</div>

